{
  "Serilog": {
    "Using": [ "Serilog.AspNetCore" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "Key": "SuperSecretKeyForDevelopment12345678901234567890",
    "Issuer": "GameAuthApi",
    "Audience": "GameClient",
    "ExpirationHours": 24
  },
  "SeedData": {
    "Enabled": true
  }
}